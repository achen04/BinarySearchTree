        bst2.print_tree();
        cout << "Removing 5 from bst, but not bst2.\n";
        bst.remove(5); // necessary to ensure that the trees
                        // aren't pointing at each other's data
        bst.print_tree();                                      
        bst2.print_tree(); // should be original               

        return 0;
